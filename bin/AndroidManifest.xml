<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myfirstapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="QuickFit"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.myfirstapp.MainActivity"
            android:label="QuickFit" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.myfirstapp.DisplayMessageActivity"
            android:label="Random Workout"
            android:parentActivityName="com.example.myfirstapp.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.myfirstapp.MainActivity" />
        </activity>
        <activity
            android:name="com.example.myfirstapp.Workout"
            android:label="@string/title_activity_workout" >
        </activity>
        <activity
            android:name="com.example.myfirstapp.Workouts"
            android:label="@string/title_activity_workouts" >
        </activity>
        <activity
            android:name="com.example.myfirstapp.GenWorkout"
            android:label="@string/title_activity_gen_workout" >
        </activity>
        <activity
            android:name="com.example.myfirstapp.GenExercise"
            android:label="@string/title_activity_gen_exercise" >
        </activity>
        <activity
            android:name="com.example.myfirstapp.GenSchema"
            android:label="@string/title_activity_gen_schema" >
        </activity>
        <activity
            android:name="com.example.myfirstapp.GenTester"
            android:label="@string/title_activity_gen_tester" >
        </activity>
    </application>

</manifest>